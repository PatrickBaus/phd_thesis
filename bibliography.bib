@article{popcorn_noise_orgin,
	abstract = {A mechanism related to crystallographic defects has been proposed by Hsu, Whittier, and Mead to explain the characteristics of burst (popcorn) noise. This paper extends their results to include sources of burst noise both in bulk material and more importantly at the surface. Experimental results are presented on specially fabricated transistors, some of which were gold doped. Experimental data are interpreted in order to substantiate the proposed burst noise model.
R{\'e}sum{\'e}
Un m{\'e}canisme se rapportant aux d{\'e}fauts de cristallographie a {\'e}t{\'e} propos{\'e} par Hsu, Whittier et Mead pour expliquer les caract{\'e}ristiques du bruit d'{\'e}clatement (popcorn). Ce document {\'e}largit ses r{\'e}sultats pour comprendre les sources de bruits d'{\'e}clatement dans les mat{\'e}riaux en vrac et plus sp{\'e}cialement en surface. Les r{\'e}sultats pr{\'e}sent{\'e}s sont ceux d'exp{\'e}riences sur des transistors fabriqu{\'e}s sp{\'e}cialement, certains {\'e}tant recouverts d'or. Les donn{\'e}es exp{\'e}rimentales sont interpr{\'e}t{\'e}es de mani{\`e}re {\`a} substancier le mod{\`e}le propos{\'e} de bruit d'{\'e}clatement.
Zusammenfassung
Zur Erkl{\"a}rung des burst-(popcorn)-Rauschens wurde von Hsu, Whittier und Mead ein Mechanismus vorgeschlagen, der mit kristallographischen Defekten verkn{\"u}pft ist. Die vorliegende Arbeit erg{\"a}nzt diese Ergebnisse unter Einschlu{\ss} von burst-Rauschquellen im Volumen und vor allem an der Halbleiteroberfl{\"a}che. Es werden experimentelle Ergebnisse an speziell hergestellten Transistoren mitgeteilt, die z.T. mit Gold dotiert waren. Die gewonnenen Daten werden interpretiert, um das vorgeschlagene Rauschmodell zu beweisen.},
	author = {Cook, K.B. and Brodersen, A.J.},
	doi = {10.1016/0038-1101(71)90112-2},
	issn = {0038-1101},
	journal = {Solid-State Electronics},
	number = {12},
	pages = {1237--1242},
	title = {Physical origins of burst noise in transistors},
	url = {https://www.sciencedirect.com/science/article/pii/0038110171901122},
	volume = {14},
	year = {1971}
}

